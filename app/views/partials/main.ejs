<% if (state === 'authenticate') { %>

  <%- include('authenticate') %>

<% } else if (state === 'profile') { %>

  <%- include('profile') %>

<% } else if (state === 'createExercise') { %>

  <%- include('createExercise') %>

<% } else if (state === 'exerciseDetail') { %>

  <%- include('detail.ejs') %>

<% } else if (state === 'createWorkout') { %>

  <%- include('createWorkout') %>

<% } else if (state === 'logWorkout') { %>

  <%- include('log') %>

<% } else { %>

  <p>A state error has occured.</p>

<% } %>
